#include <stdio.h>
#include <stdlib.h>
#include <math.h>

///Converting any negative number to positive number
int main()
{
    int n1, n2;

    printf("Enter two integers: ");
    scanf("%d %d",&n1,&n2);

    /// if user enters negative number, sign of the number is changed to positive
    n1 = ( n1 > 0) ? n1 : -n1;
    n2 = ( n2 > 0) ? n2 : -n2;
    printf("%d\n",n1);
    printf("%d\n",n2);

    return 0;
}
