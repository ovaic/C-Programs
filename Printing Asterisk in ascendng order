///#1
///Printing Asterisk in ascendng order
/*
*
**
***
****
*****
******
*/

#include <stdio.h>
#include <stdlib.h>

int main()                                    
{                                             
    int i,j;                                  
    for(i=1;i<=10;i++)
    {
        for(j=1;j<=i;j++)
        {
            printf("*");
        }
        printf("\n");
    }
    return 0;
}
