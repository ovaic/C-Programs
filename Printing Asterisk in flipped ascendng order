///Asterisk in ascendng order (flipped)
/*
    *
   **
  ***
 ****
*****
*/
#include <stdio.h>
#include <stdlib.h>

int main()
{
    int i,j,s;
    for(i=1;i<=10;i++)
    {
        for (s=10;s>=i;s--)
        {
            printf(" ");
        }
        for(j=1;j<=i;j++)
        {
            printf("*");
        }
        printf("\n");
    }
    return 0;
}
