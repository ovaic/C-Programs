///3
///Printing Asterisk in descendng order (FLIPPED)
/*
******
 *****
  ****
   ***
    **
     *
*/
#include <stdio.h>
#include <stdlib.h>

int main()
{
    int i,j,s;
    for(i=1;i<=10;i++)
    {
        for(s=0;s<i;s++)
        {
            printf(" ");
        }
         for(j=10;j>=i;j--)
        {
            printf("*");
        }
        printf("\n");

    }
    return 0;
}
