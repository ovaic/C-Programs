#include <stdio.h>

#include <stdlib.h>



///ASCIIconverter.c

/// ASCII value for a given character:



int main()

{

    int e;

    char ch;



    printf("\n Enter a character : ");

    scanf("%c",&ch);

    e=ch;

    printf("\n The ASCII value of the character is : %d",e);

    getchar();

    return 0;

}
